<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#F0F1F5" />
            <stroke
                android:width="1dp"
                android:color="#d9dbe7" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="6dp"
                android:top="6dp"
                android:right="6dp"
                android:bottom="6dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#F0F1F5"
                android:endColor="#d9dbe7"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#d9dbe7" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="6dp"
                android:top="6dp"
                android:right="6dp"
                android:bottom="6dp" />
        </shape>
    </item>
</selector>